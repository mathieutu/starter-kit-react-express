{
  "name": "starter-kit-react-express",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "react": "^16.8.0-alpha.1",
    "react-dom": "^16.8.0-alpha.1",
    "react-router-dom": "^4.3.1",
    "serve-my-app": "*"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "express": "serve-my-app --watch --srv=dist",
    "express:run": "NODE_ENV=production serve-my-app --srv=dist",
    "build": " npm run build:src && npm run build:srv",
    "build:src": "rm -rf build && react-scripts build",
    "build:srv": "rm -rf dist && tsc -p srv",
    "watch:srv": "npm run build:srv -- -w",
    "lint": "tslint --project ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/jest": "23.3.13",
    "@types/node": "10.12.21",
    "@types/react": "16.8.1",
    "@types/react-dom": "16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-react": "^3.6.0",
    "typescript": "3.3.1"
  }
}
