{
  "name": "starter-kit-react-express",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.17.3",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-router-dom": "^4.3.1",
    "serve-my-app": "^0.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": " npm run build:src && npm run build:srv",
    "build:src": "rm -rf build && react-scripts build",
    "build:srv": "rm -rf dist && tsc -p srv",
    "lint": "tslint --project .",
    "sma:watch": "serve-my-app --srv=srv --proxify --watch",
    "sma": "serve-my-app --srv=srv --src=build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/jest": "23.3.13",
    "@types/node": "10.12.21",
    "@types/react": "16.8.1",
    "@types/react-dom": "16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "react-scripts": "2.1.8",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-react": "^3.6.0",
    "typescript": "3.3.1"
  }
}
